
{% if not packages == "NOT FOUND" %}
    {% for pkg in packages %}
        <a href="{{ url_for('simple.get_package', package=pkg.fullname) }}">{{ pkg.fullname }}</a><br />
    {% endfor %}
    {% for sc in scrap %}
        <a href="{{ url_for('simple.download_package', package=sc[1], href=sc[0]) }}">{{ sc[1] }}</a><br />
    {% endfor %}
{% else %}
    <h2 style="color: red">{{packages}}</h2>
{% endif %}
